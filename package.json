    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "memory",
    "mindos",
    "claude",
    "postgres",
    "chat"
