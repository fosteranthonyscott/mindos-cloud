{
  "name": "mindos-cloud",
  "version": "2.0.0",
  "description": "MindOS - Personal AI Assistant with Chat Interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node migrate.js",
    "migrate-enhanced": "node migrate-enhanced-memory.js",
    "db:cleanup": "node -e \"require('./migrate-enhanced-memory.js').runCleanup()\"",
